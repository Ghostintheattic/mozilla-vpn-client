    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug: Client",
            "osx": {
                "program": "${workspaceFolder}/.vscode/build_osx/src/Mozilla VPN.app/Contents/MacOS/Mozilla VPN",
            },
            "windows": {
                "type": "cppvsdbg",
                "request": "launch",
                "program": "C:/Program Files/Mozilla/Mozilla VPN/MozillaVPN.exe",
                "symbolSearchPath": "${workspaceFolder}/.vscode/",
                "args": [],            
                "stopAtEntry": false,
            },
          
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug: Tests",
            "program": "${workspaceFolder}/.vscode/build_osx/tests/unit/unit_tests",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Debug: Taskcluster Taskgraph",
            "program": "${workspaceFolder}/.vscode/task_scripts/launch_taskgraph.py",
            "args": [
                "full",
                "-p",
                "taskcluster/test/params/main-push.yml"
            ],
            "cwd": "${workspaceFolder}"
        },
    ]
}